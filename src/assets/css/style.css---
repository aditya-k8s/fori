.number-input input[type=number]::-webkit-inner-spin-button,.number-input input[type=number]::-webkit-outer-spin-button{-webkit-appearance:none}
.number-input button{-webkit-appearance:none;background-color:transparent;border:none;-webkit-box-align:center;-ms-flex-align:center;align-items:center;-webkit-box-pack:center;-ms-flex-pack:center;justify-content:center;cursor:pointer;margin:0;position:relative}
.number-input button:before,.number-input button:after{display:inline-block;position:absolute;content:'';height:2px;-webkit-transform:translate(-50%,-50%);transform:translate(-50%,-50%)}
.number-input button.plus:after{-webkit-transform:translate(-50%,-50%) rotate(90deg);transform:translate(-50%,-50%) rotate(90deg)}
