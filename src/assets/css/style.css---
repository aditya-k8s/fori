.check-btn-sap {
cursor: pointer;-webkit-user-select: none;-moz-user-select: none;-ms-user-select: none;user-select: none;position: absolute;top: 20px;right: 45px;
color: #fff !important;text-decoration: none;background-color: transparent;text-shadow: 0 0 5px #00000087;box-shadow: 0 0;border: 0;font-size: 16px; margin: 0 !important;}
/* Hide the browser's default checkbox */
.check-btn-sap input {  position: absolute;  opacity: 0;  cursor: pointer;  height: 0;  width: 0;}
/* Create a custom checkbox */
.check-btn-sap .checkmark {  position: absolute;  top: 0;  left: 0;  height: 25px;  width: 25px;  background-color: #eee;  border-radius: 5px;box-shadow: 0 0 2px 1px #00000036;}
/* On mouse-over, add a grey background color */
.check-btn-sap:hover input ~ .checkmark {  background-color: #e0e0e0;}
/* When the checkbox is checked, add a blue background */
.check-btn-sap input:checked ~ .checkmark {  background-color: #e25751;}
/* Create the checkmark/indicator (hidden when not checked) */
.check-btn-sap .checkmark:after {  content: "";  position: absolute;  display: none;}
/* Show the checkmark when checked */
.check-btn-sap input:checked ~ .checkmark:after {  display: block;}
/* Style the checkmark/indicator */
.check-btn-sap .checkmark:after {  left: 10px;  top: 7px;  width: 5px;  height: 10px;  border: solid white;  border-width: 0 3px 3px 0;  -webkit-transform: rotate(45deg);
-ms-transform: rotate(45deg);  transform: rotate(45deg);}
.check-btn-sap * {    transition: all .3s ease-in-out;}

